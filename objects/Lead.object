    <validationRules>
        <fullName>DeclineReasonRequired_LeadDeclined</fullName>
        <active>true</active>
        <description>Vertiba3/10/2014 - Phase1 - The &quot;declined reason&quot; is required when the lead has been declined</description>
        <errorConditionFormula>IF( 
AND( 
ISPICKVAL(Status,&quot;Declined&quot;), 
ISBLANK(TEXT(RES_Declined_Reason__c)) 
), 
true, 
false 
)</errorConditionFormula>
        <errorMessage>Please select the declined reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Distributed_Account_Type</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL(Account_Type__c,&quot;&quot;),
$Profile.Id =&apos;00ei0000001lsec&apos; )</errorConditionFormula>
        <errorDisplayField>Account_Type__c</errorDisplayField>
        <errorMessage>Please enter Account Type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lead_Media_Required</fullName>
        <active>true</active>
        <description>Vertiba3/10/2014 - Phase1 - Created to make either email, phone, or mobile required</description>
        <errorConditionFormula>AND( 
ISBLANK(Phone), 
ISBLANK(Email), 
ISBLANK(MobilePhone),
$Profile.LastModifiedById  = &quot;00ei0000001BR5D&quot;,
IsConverted
)</errorConditionFormula>
        <errorMessage>Please enter either the Phone, Email or Mobile.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lead_Opportunity_Required</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
ISBLANK(ConvertedOpportunityId),
$Profile.Id =&apos;00ei0000001lsec&apos;,
IsConverted )</errorConditionFormula>
        <errorMessage>In Order to Convert this Lead you must create an Opportunity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lead_Source_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL(LeadSource,&quot;&quot;),
IsConverted )</errorConditionFormula>
        <errorMessage>In Order to Convert this Lead you must go back the Lead, and input a &apos;Lead Source&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OtherLeadSource</fullName>
        <active>true</active>
        <description>Vertiba 04/22/2014 Phase1- Created to track Other Lead Source if Lead Source=Other</description>
        <errorConditionFormula>AND(
ISPICKVAL(LeadSource,&quot;Other&quot;),
ISBLANK(Lead_Source_Notes__c),
IsConverted
)</errorConditionFormula>
        <errorMessage>Please populate the &quot;Other Lead Source&quot;</errorMessage>
    </validationRules>
