    <validationRules>
        <fullName>Closed_Won</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL(Stage_Phase__c, &quot;Closed Won&quot;),
ISPICKVAL(Opportunity__r.Outcome_Reason__c, &quot;&quot;)
)</errorConditionFormula>
        <errorMessage>Please indicated Closed Outcome in the Parent Opportunity before marking this PPA Bid as Closed Won.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Downside_Risk_Protection_is_YesOther</fullName>
        <active>true</active>
        <description>If  &apos;Downside Protection (DSP) -Type&apos; = &apos;Yes- Other&quot;, then the User must input a value in the &apos;DSP Type -Other&apos; field</description>
        <errorConditionFormula>AND(
ISPICKVAL(Downside_Protection_Type__c,&quot;Yes- Other&quot;),
ISBLANK(DSP_Type_Other__c))</errorConditionFormula>
        <errorDisplayField>DSP_Type_Other__c</errorDisplayField>
        <errorMessage>You selected &apos;Yes- Other&apos; for the &apos;Downside Protection (DSP) -Type&apos;  field. Please input a value in the &apos;DSP Type -Other&apos; field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_one_Bid_Selected_Checked</fullName>
        <active>true</active>
        <errorConditionFormula>Opportunity__r.Selected_Bid_Count__c &gt; 1</errorConditionFormula>
        <errorDisplayField>Selected_Bid__c</errorDisplayField>
        <errorMessage>There is already another Opportunity with the &apos;Bid Selected&apos; field checked. If you would like to change which Project is mapped to this Opportunity, please navigate back to the &quot;Project / Opportunity&quot; and uncheck the appropriate record.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
