    <validationRules>
        <fullName>Amount_Cannot_be_0</fullName>
        <active>true</active>
        <description>NOT(ISPICKVAL( Opportunity_Type__c ,&quot;PPA&quot;)),
ConvertedFromLead__c=False,</description>
        <errorConditionFormula>AND(
INCLUDES(RES_Country__c ,&quot;United States&quot;),
NOT(ISPICKVAL(StageName,&quot;Prospecting&quot;)),
NOT(ISPICKVAL(StageName,&quot;Pending RFP&quot;)),
NOT(ISPICKVAL(StageName,&quot;Closed - DNB (did not bid)&quot;)),
NOT(RecordTypeId = &quot;0120H000001QTGf&quot;),
$User.ProfileId &lt;&gt; &quot;00ei0000001BR5D&quot;,
Amount&lt;1)</errorConditionFormula>
        <errorDisplayField>Amount</errorDisplayField>
        <errorMessage>Amount cannot be $0, please estimate a Contract Value for this Opportunity and input that figure in one of the BL Revenue fields.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Close_Date_Cannot_Equal_COD</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$User.ProfileId &lt;&gt; &quot;00ei0000001BR5D&quot;,
IF(CloseDate =  COD_Date__c, true,
IF(CloseDate =  Construction_Completion_Date__c , true,
IF(CloseDate =  Construction_Start__c, true,
false
)))
)</errorConditionFormula>
        <errorMessage>Close Date cannot be the same as value for either the &apos;COD Date&apos;, the &apos;Construction Completion Date&apos;, or the &apos;Construction Start Date&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Close_dates_in_Past</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
IsClosed = False,
NOT(ISCHANGED(IsClosed)), 
NOT(ISPICKVAL(StageName,&quot;Tabled&quot;)),
CloseDate &lt; Today(),
$User.ProfileId &lt;&gt; &quot;00ei0000001BR5D&quot;
)</errorConditionFormula>
        <errorMessage>The Close Date is in the past and this Opportunity is still Open. Please update the Close Date or the Opportunity Stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Won_must_enter_Closed_Status</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
ISPICKVAL(Closed_Status__c,&quot;&quot;),
ISPICKVAL(StageName  , &quot;Closed Won&quot;),
$User.ProfileId &lt;&gt; &quot;00ei0000001BR5D&quot;
)</errorConditionFormula>
        <errorDisplayField>Closed_Status__c</errorDisplayField>
        <errorMessage>Must input a value for Closed Status, as this Opportunity is Closed Won.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Won_must_input_Project_Work_Order</fullName>
        <active>false</active>
        <description>De-activated 
Excludes Origination Opportunities &amp; T&amp;D User Profiles / Record Types</description>
        <errorConditionFormula>AND(
INCLUDES(RES_Country__c ,&quot;United States&quot;),
ISCHANGED(StageName),
ISPICKVAL(StageName ,&quot;Closed Won&quot;),
ISBLANK( Project_Number__c),
$User.ProfileId &lt;&gt; &quot;00ei0000001BR5D&quot;,
$User.ProfileId &lt;&gt; &quot;00ei0000001lsG6&quot;,
$User.Id &lt;&gt; &quot;0050H00000B7L9X&quot;,
$User.Id &lt;&gt; &quot;005i0000007Op46&quot;,
$User.Id &lt;&gt; &quot;005i0000007QDO9&quot;,
$User.Id &lt;&gt; &quot;005i0000007Oy0m&quot;,
$User.Id &lt;&gt; &quot;0050H0000094eM5&quot;,
$User.Id &lt;&gt; &quot;005i0000007QDOd&quot;,
$User.Id &lt;&gt; &quot;0056N0000016igr&quot;,
$User.Id &lt;&gt; &quot;005i0000006IdKl&quot;,
$User.Id &lt;&gt; &quot;0050H00000CWzjF&quot;,
$User.Id &lt;&gt; &quot;0050H00000BKILM&quot;,
RecordTypeId &lt;&gt; &quot;0120H000001QTGf&quot;,
RecordTypeId &lt;&gt; &quot;0120H000001cqxr&quot;
)</errorConditionFormula>
        <errorDisplayField>Project_Number__c</errorDisplayField>
        <errorMessage>To move this Opportunity to &quot;Closed Won&quot;, please first input a value in the &quot;Project/Work Order #&quot; field, that corresponds to Business World&apos;s project codes.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>France_Invoiced_Validation</fullName>
        <active>true</active>
        <description>Makes sure the &quot;To Be Invoiced&quot; and &quot;Invoiced&quot; checkboxes aren&apos;t selected unless StageName = &quot;Closed Won&quot;</description>
        <errorConditionFormula>AND(OR(Invoiced__c=TRUE, To_Be_Invoiced__c=TRUE),  NOT(ISPICKVAL(StageName, &quot;Closed Won&quot;)))</errorConditionFormula>
        <errorMessage>&quot;To Be Invoiced&quot; and &quot;Invoiced&quot; checkboxes cannot be selected unless the Stage is &quot;Closed Won&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>France_Multiple_Invoice_Selections</fullName>
        <active>true</active>
        <description>Ensures that the &quot;To Be Invoiced&quot; and &quot;Invoiced&quot; fields are not both selected at the same time.</description>
        <errorConditionFormula>AND(  To_Be_Invoiced__c=TRUE  ,  Invoiced__c=TRUE  )</errorConditionFormula>
        <errorMessage>&quot;To Be Invoiced&quot; and &quot;Invoiced&quot; may not both be selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Gross_Profit_must_be_entered</fullName>
        <active>true</active>
        <description>NOT(ISPICKVAL(Opportunity_Type__c ,&quot;PPA&quot;)),</description>
        <errorConditionFormula>AND(
INCLUDES(RES_Country__c ,&quot;United States&quot;),
ISBLANK(Gross_Profit__c),
ISBLANK(T_D_Margin__c),
ISBLANK( Wind_Constr_Margin__c ),
ISBLANK( Storage_Margin__c ),
ISBLANK( Solar_Constr_Margin__c ),
ISBLANK(  Dev_Margin__c ),
$User.ProfileId &lt;&gt; &quot;00ei0000001BR5D&quot;,
NOT(RecordTypeId = &quot;0120H000001QTGf&quot;),
Probability&gt;0.30
)</errorConditionFormula>
        <errorDisplayField>Gross_Profit__c</errorDisplayField>
        <errorMessage>Please estimate a value for &apos;Gross Profit&apos;. (This Oppty has progressed far enough down the Sales Stages, so please input a Gross Profit figure in one of the BL Profit fields.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Important_Dates_are_Incorrect</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
INCLUDES(RES_Country__c ,&quot;United States&quot;),
$User.ProfileId &lt;&gt; &quot;00ei0000001BR5D&quot;,
OR(
CloseDate &gt;= COD_Date__c, 
CloseDate &gt;= Construction_Completion_Date__c,
AND(NOT(ISBLANK(COD_Date__c)),
  Construction_Start__c &gt;= COD_Date__c),
AND(NOT(ISBLANK(Construction_Completion_Date__c)),
Construction_Start__c &gt;= Construction_Completion_Date__c)
))</errorConditionFormula>
        <errorDisplayField>CloseDate</errorDisplayField>
        <errorMessage>Please correct your dates: either Close Date, Construction Start, COD, or Substantial Completion Date are incorrect.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Input_Size</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
INCLUDES(RES_Country__c ,&quot;United States&quot;),
ISBLANK( Size__c ),
ISBLANK( Size_MWdc__c),
ISBLANK( Size_MWh__c ),
BL_Sum__c &lt;&gt; Transmission_Revenue__c,
$User.ProfileId &lt;&gt; &quot;00ei0000001BR5D&quot;,
NOT(ISCHANGED(of_PPA_Bid_Prices__c)),
OR(
ISPICKVAL(StageName,&quot;Negotiations&quot;),
ISPICKVAL(StageName,&quot;Preferred Supplier/NOA&quot;),
ISPICKVAL(StageName,&quot;Shortlist&quot;),
ISPICKVAL(StageName,&quot;Closed Won&quot;),
ISPICKVAL(StageName,&quot;Closed Lost&quot;))
)</errorConditionFormula>
        <errorMessage>Now that this Opportunity has progressed into one of the late stages, you must estimate a value for at least one of the following 4 fields: &apos;Size (MWac)&apos;,  &apos;Size (MWdc)&apos;, or  &apos;Size (MWh)&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP_VR01_PercentFieldLessThan0</fullName>
        <active>true</active>
        <errorConditionFormula>OR( Percent_Collection__c &lt;0, Percent_Distribution__c &lt;0, Percent_Storage__c &lt;0, Percent_Substation__c &lt;0, Percent_Transmission__c &lt;0)</errorConditionFormula>
        <errorMessage>&quot;T&amp;D Percentage Fields Cannot Be Less Than 0&quot; [OPP_VR01]</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP_VR02_PercentCannotBeOver100</fullName>
        <active>true</active>
        <errorConditionFormula>Percent_Unallocated__c &lt; 0</errorConditionFormula>
        <errorMessage>&quot;Sum of the T&amp;D % fields cannot exceed 100&quot; [OPP_VR02]</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Origination_Broker_Account</fullName>
        <active>true</active>
        <description>Upon Creation, Origination users must enter either &apos;Broker Account&apos;, &apos;Broker Contact&apos;, or the Broker checkbox if there was no Broker associated to this Opportunity.</description>
        <errorConditionFormula>AND(
RecordTypeId = &quot;0120H000001QTGf&quot;,
$User.ProfileId = &quot;00ei0000001NeHn&quot;,
ISNEW(),
ISBLANK(Broker_Account__c),
ISBLANK(Broker_Contact__c ),
Broker_Audit__c = False
)</errorConditionFormula>
        <errorDisplayField>Broker_Contact__c</errorDisplayField>
        <errorMessage>Please enter a Broker, or check the &quot;Broker Audit&quot; box to indicate there is no broker for this Opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Origination_Latest_Update</fullName>
        <active>true</active>
        <description>For Origination Opportunities, users must update the &quot;Latest Update&quot; field everytime they change the Stage.</description>
        <errorConditionFormula>AND(
RecordTypeId = &quot;0120H000001QTGf&quot;,
$User.ProfileId = &quot;00ei0000001NeHn&quot;,
ISCHANGED(StageName),
NOT(ISCHANGED(Latest_Update__c )),
Update_Date__c &lt; TODAY() - 5
)</errorConditionFormula>
        <errorDisplayField>Latest_Update__c</errorDisplayField>
        <errorMessage>Please enter a new Latest Update.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Origination_Proposal_Preparation_Fields</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
RecordTypeId = &quot;0120H000001QTGf&quot;,
$User.ProfileId = &quot;00ei0000001NeHn&quot;,
ISPICKVAL(StageName, &quot;Proposal Preparation&quot;),
OR(
ISBLANK(RES_Technology_Type__c ),
ISBLANK(Proposal_Due_Date__c ),
ISPICKVAL(Origination_Bid_Structure__c,&quot;&quot;),
ISBLANK(COD_Specifications__c),
ISBLANK(RTO_Region__c ),
AND(
ISBLANK(Size__c),
ISBLANK(Size_MWh__c)
)
))</errorConditionFormula>
        <errorMessage>Please reference the &quot;Proposal Specifications&quot; Section.
Required fields to move onto the Proposal Preparation stage are:
Origination Bid Structure, Proposal Due Date, COD Specifications, RTO Region, Technology Type, &amp; Size (MW)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Origination_Proposal_Preparation_RFP</fullName>
        <active>true</active>
        <description>If an Origination Opportunity is an RFP, &quot;RFP Issued Date&quot; must be entered.</description>
        <errorConditionFormula>AND(
RecordTypeId = &quot;0120H000001QTGf&quot;,
$User.ProfileId = &quot;00ei0000001NeHn&quot;,
ISPICKVAL(StageName, &quot;Proposal Preparation&quot;),
ISPICKVAL(Origination_Bid_Structure__c,&quot;RFP&quot;),
ISBLANK(RFP_Issued_Date__c )
)</errorConditionFormula>
        <errorDisplayField>RFP_Issued_Date__c</errorDisplayField>
        <errorMessage>Please enter RFP Issued Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OutcomeReasonRequired_Oppty_Closed</fullName>
        <active>false</active>
        <description>Vertiba3/10/2014 - Phase1 - The &quot;outcome reason&quot; is required when the oppty has been Closed</description>
        <errorConditionFormula>AND(
IsClosed = TRUE,
ISCHANGED(IsClosed),
ISBLANK(TEXT(Outcome_Reason__c)),
$User.ProfileId &lt;&gt; &quot;00ei0000001BR5D&quot;
)</errorConditionFormula>
        <errorDisplayField>Outcome_Reason__c</errorDisplayField>
        <errorMessage>Please select the Outcome Reason, now that this Opportunity has been closed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Size_3_Units_Validation_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISBLANK(TEXT(Size_3_Units__c )), 
Size_3__c &gt;0
)</errorConditionFormula>
        <errorDisplayField>Size_3_Units__c</errorDisplayField>
        <errorMessage>Please indicate the type of Units for this figure within the &quot;Size 3 Units&quot; field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Solar_Required_Fields_for_Procurement</fullName>
        <active>false</active>
        <description>Required fields for Tomaso in Solar Procurement.

ConvertedFromLead__c=False,</description>
        <errorConditionFormula>AND(
Solar_Constr_Revenue__c &gt;0,
INCLUDES( Business_Line__c, &quot;Solar Const&quot;),
OR(
ISBLANK(Construction_Start__c ),
ISPICKVAL(State_Province__c,&quot;&quot;),
ISBLANK(Location__c ),
ISPICKVAL(Fixed_Track__c,&quot;&quot;)
),
$User.ProfileId &lt;&gt; &quot;00ei0000001BR5D&quot;,
RecordTypeId&lt;&gt;&quot;0120H000001QTGf&quot;,
Probability&gt;0.32
)</errorConditionFormula>
        <errorMessage>For Solar Procurement, the following 4 fields are mandatory: &apos;State/Province&apos;, &apos;Location&apos;, &apos;Construction Start Date&apos;, and &apos;Fixed/Track&apos;.

(This Opportunity has progressed far enough down the Sales Stages, so now these 4 fields are required).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Update_COD_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
INCLUDES(RES_Country__c ,&quot;United States&quot;),
Probability &gt;=  0.70,
ISBLANK(COD_Date__c ),
ISBLANK(Construction_Completion_Date__c),
ISBLANK(Final_Completion_Date__c),
ISBLANK(Contract_Start_Date__c), 
$User.ProfileId &lt;&gt; &quot;00ei0000001BR5D&quot;,
NOT(ISPICKVAL( Contract_Type__c ,&quot;PPA&quot;))
)</errorConditionFormula>
        <errorDisplayField>COD_Date__c</errorDisplayField>
        <errorMessage>Please input a value for either &apos;COD Date&apos; .  &quot;Substantial Completion Date&apos; , or the &apos;Contract Start Date&apos; field (now that the Oppty has progressed far enough down the sales funnel)</errorMessage>
    </validationRules>
