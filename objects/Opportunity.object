        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(Account.Account_Type__c , &quot;IOU&quot;), &quot;Public Power&quot;,
IF(ISPICKVAL(Account.Account_Type__c , &quot;IPP&quot;), &quot;Public Power&quot;,
IF(ISPICKVAL(Account.Account_Type__c , &quot;Municipal (MOU)&quot;), &quot;Public Power&quot;,
IF(ISPICKVAL(Account.Account_Type__c , &quot;Coop&quot;), &quot;Public Power&quot;,
IF(ISPICKVAL(Account.Account_Type__c , &quot;DistCoop&quot;), &quot;Public Power&quot;,
IF(ISPICKVAL(Account.Account_Type__c , &quot;Non-Muni Govt&quot;), &quot;Public Power&quot;,
IF(ISPICKVAL(Account.Account_Type__c , &quot;CCA&quot;), &quot;Public Power&quot;,
IF(ISPICKVAL(Account.Account_Type__c , &quot;C&amp;I&quot;), &quot;C&amp;I&quot;,
IF(ISPICKVAL(Account.Account_Type__c , &quot;University&quot;), &quot;C&amp;I&quot;,
IF(ISPICKVAL(Account.Account_Type__c , &quot;Developer&quot;), &quot;C&amp;I&quot;,
IF(ISPICKVAL(Account.Account_Type__c , &quot;Trading Desk&quot;), &quot;C&amp;I&quot;,
IF(ISPICKVAL(Account.Account_Type__c , &quot;REC Broker&quot;), &quot;C&amp;I&quot;,
IF(ISPICKVAL(Account.Account_Type__c , &quot;Power Broker&quot;), &quot;C&amp;I&quot;,
IF(ISPICKVAL(Account.Account_Type__c , &quot;Non-Muni Govt&quot;), &quot;C&amp;I&quot;,&quot;&quot;
))))))
)))))
)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
