    <validationRules>
        <fullName>Task_Completed_in_Future</fullName>
        <active>true</active>
        <description>A Task cannot have its status changed to &quot;Completed/closed&quot; if the Activity Date is listed in the future.</description>
        <errorConditionFormula>AND( ISPICKVAL(Status, &quot;Completed/closed&quot;),  ActivityDate &gt;  TODAY() )</errorConditionFormula>
        <errorMessage>A Task cannot be set to Completed/closed if the Due Date is listed in the future.</errorMessage>
    </validationRules>
